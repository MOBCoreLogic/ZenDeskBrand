function determineCategoryFormMapping() {
  let setting = "{{settings.category_form_mapping}}"
  if (setting) {
    try {
      return JSON.parse(setting.replaceAll('&quot;', '\"'))
    } catch (e) {
      console.error('category_form_mapping not propery formatted - form mapping will not operate as expected')
      return {}
    }
  } else {
    return {}
  }
}

function determineCategoryGroupMembers(index) {
  let setting
  switch (index) {
    case 1:
      setting = '{{settings.category_group_members_1}}'
      break;
    case 2:
      setting = '{{settings.category_group_members_2}}'
      break;
    case 3:
      setting = '{{settings.category_group_members_3}}'
      break;
    case 4:
      setting = '{{settings.category_group_members_4}}'
      break;
    case 5:
      setting = '{{settings.category_group_members_5}}'
      break;
  }
  if (setting && setting.trim()) {
    try {
      return setting.split(',').map(member => member.trim())
    } catch (e) {
      console.error(`issue with category_group_members_${index} - category group mapping failed`)
      return []
    }
  } else {
    return {}
  }
}



